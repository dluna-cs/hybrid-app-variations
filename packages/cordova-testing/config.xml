<?xml version='1.0' encoding='utf-8'?>
<widget id="com.contentsquare.cordova.testapp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>testing-app</name>
    <description>testing-app</description>
    <author email="mobile@contentsquare.com">Contentsquare Mobile Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="BackupWebStorage" value="none" />
    <platform name="android">
        <preference name="Scheme" value="http" />
        <preference name="android-targetSdkVersion" value="31" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <preference name="deployment-target" value="12" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
